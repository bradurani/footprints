{
  "parserOptions": {
    "ecmaVersion": 3,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": false
  },
  "globals": {
    "document": true,
    "window": true,
    "navigator": true
  },
  "rules": {
    "comma-dangle": "error",
    "no-extend-native": "error",
    "semi": "error",
    "strict": 0,
    "no-undef": "error"
  }
}
